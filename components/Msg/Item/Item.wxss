/* Components/Msg/Item/Item.wxss */

.team-apl {
  margin-top: 35rpx;
  /*margin-bottom: 25rpx;*/
}

.apl-header {
  display: flex;
  flex: row;
  justify-content: space-between;
  margin-bottom: 15rpx;
}

.apl-info {
  display: flex;
  flex-direction: row;
  align-items: center;
}

.apl-info-avatar {
  width: 75rpx;
  height: 75rpx;
  border-radius: 50%;
  box-shadow: #aaa 0rpx 1rpx 50rpx 0rpx;
}

.apl-info-name {
  display: flex;
  flex-direction: column;
  /*justify-content: space-around;*/
  align-items: flex-start;
  margin-left: 20rpx;
  color: black;
  font-size: 30rpx;
}

.apl-info-name-append {
  color: #aaa;
  font-size: 85%;
  line-height: 85%;
}

.apl-info-btn {
  margin: 0;
  height: 75rpx;
  border-radius: 37.5rpx;
  box-shadow: #ff8080 0rpx 1rpx 50rpx 0rpx;
  background-color: #ff8080;
  color: white;
  font-size: 30rpx;
  line-height: 75rpx;
}

.apl-info-btn-hidden {
  display: none;
}

.apl-info-btn-default {
  box-shadow: #aaa 0rpx 1rpx 50rpx 0rpx;
  background-color: #aaa;
}

.apl-desc {
  padding: 10rpx 20rpx; 
  background-color: #F9F8FF;
}

.apl-contact {
  display: flex;
  flex-direction: row;
  align-items: center;
}

.apl-contact-item {
  width: 35rpx;
  height: 35rpx;
  margin-left: 20rpx;
}

