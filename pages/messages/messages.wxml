      <view class="teamContainer">

        <view class="team-area">
          <view class="team-area-title">作为创建者</view>
          <view class="team-area-content team-desc">
            <msg-list msgs="{{ msgs_pub }}" category="public" />
          </view>
        </view>

        <view class="team-area">
          <view class="team-area-title">作为申请者</view>
          <view class="team-area-content team-desc">
            <msg-list msgs="{{ msgs_apl }}" category="apply" />
          </view>
        </view>
      
      </view>

    </view>
</view>



